create_table("partner") {
  t.Column("id", "integer", {primary: true})
  t.Column("first_name", "string", {"default": ""})
  t.Column("last_name", "string", {"default": ""})
  t.Column("email", "string", {})
  t.Column("rating", "integer")
  t.Column("address_lat", "integer")
  t.Column("address_lon", "integer")
  t.Column("operating_radius", "integer")
}

add_index("partner", "email", {"unique": true})